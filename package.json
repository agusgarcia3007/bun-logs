{
  "name": "bun-logs",
  "version": "0.1.3",
  "description": "Ultra-light, Bun-first async logger with batching & worker offload",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./worker": {
      "import": "./dist/worker.js",
      "types": "./dist/worker.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/agusgarcia3007/bun-logs.git"
  },
  "bugs": {
    "url": "https://github.com/agusgarcia3007/bun-logs/issues"
  },
  "homepage": "https://github.com/agusgarcia3007/bun-logs#readme",
  "scripts": {
    "build": "bun build --entrypoints src/index.ts src/worker.ts --outdir dist --target bun --minify && cp src/index.d.ts dist/index.d.ts",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "dev": "bun run examples/dev.ts",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "bun",
    "logger",
    "logging",
    "worker",
    "ndjson",
    "async"
  ],
  "author": "agusgarcia3007",
  "license": "MIT"
}
